<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
  <body>
    <%- include('./partials/header.ejs') %>
    <main class="main">
      <h3 class="titulo">Ultimos agregados</h3>
      <section class="section-html">
        <% products.forEach(producto => { %> .
        <div class="ver">
          <img class="imagenProducto" src="/img/products/<%= producto.image %> "alt="alt"/>
          <article>
            <div class="pdp">
              <p class="precio">$<%= producto.price %></p>
              <p class="producto"><%= producto.name %></p>
              <p class="descuento"><%= producto.description %></p>
            </div>
          </article>
        </div>
        <% }) %>
      </section>
    </main>
  </body>
</html>
